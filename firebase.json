{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "python311",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "__pycache__",
        "*.pyc",
        "*.pyo",
        "*.pyd",
        ".Python",
        "env",
        "venv",
        ".venv"
      ],
      "predeploy": [
        {
          "command": "python",
          "args": [
            "-m",
            "functions.devtools.ensure_venv"
          ]
        }
      ]
    }
  ]
}
