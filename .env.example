# OpenRouter API Configuration
# Get your free API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-your-api-key-here

# OpenRouter Model Selection
# Free model: openai/gpt-oss-20b:free
# Other options: meta-llama/llama-3.2-3b-instruct:free, google/gemma-2-9b-it:free
OPENROUTER_MODEL=openai/gpt-oss-20b:free

# Firebase Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id

# Cache Settings (hours)
CACHE_DURATION_HOURS=24

# Feature Flags
ENABLE_AI_PARSING=True
DEBUG=False

# AI Configuration
# Minimum number of null/empty values before triggering AI enhancement
# Default: 3 (set to 0 to always use AI, higher values to use AI less frequently)
AI_NULL_THRESHOLD=3
